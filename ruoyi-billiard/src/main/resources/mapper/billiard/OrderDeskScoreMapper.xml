<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.billiard.mapper.OrderDeskScoreMapper">


    <sql id="selectOrderDeskScoreVo">
        select  *
        from t_order_desk_score
    </sql>

    <select id="selectOrderDeskScoreList" parameterType="OrderDeskScore" resultType="OrderDeskScore">
        <include refid="selectOrderDeskScoreVo"/>
        <where>
            <if test="orderId != null ">and t_order_id = #{tOrderId}</if>
            <if test="deskId != null ">and desk_id = #{deskId}</if>
            <if test="scoreA != null ">and score_a = #{scoreA}</if>
            <if test="scoreB != null ">and score_b = #{scoreB}</if>
            <if test="startTime != null ">and start_time = #{startTime}</if>
            <if test="endTime != null ">and end_time = #{endTime}</if>
            <if test="createById != null ">and create_by_id = #{createById}</if>
            <if test="updateById != null ">and update_by_id = #{updateById}</if>
        </where>
    </select>


</mapper>