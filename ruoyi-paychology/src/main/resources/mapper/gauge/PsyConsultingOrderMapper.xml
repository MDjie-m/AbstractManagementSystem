            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deletePsyConsultingOrderById" >
        delete from psy_consulting_order where id = #{id}
    </delete>

    <delete id="deletePsyConsultingOrderByIds" parameterType="String">
        delete from psy_consulting_order where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>