            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updatePsyConsultingOrder" parameterType="PsyConsultingOrder">
        update psy_consulting_order
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="consultantId != null">consultant_id = #{consultantId},</if>
            <if test="type != null">type = #{type},</if>
            <if test="consultingTime != null">consulting_time = #{consultingTime},</if>
            <if test="subscribeTime != null">subscribe_time = #{subscribeTime},</if>
