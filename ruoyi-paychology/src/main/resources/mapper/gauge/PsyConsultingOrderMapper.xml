    <sql id="selectPsyConsultingOrderVo">
        select id, user_id, consultant_id, type, consulting_time, subscribe_time, price, status, order_num, order_time, create_by, create_time, update_by, update_time from psy_consulting_order
    </sql>

    <select id="selectPsyConsultingOrderList" parameterType="PsyConsultingOrder" resultMap="PsyConsultingOrderResult">
        <include refid="selectPsyConsultingOrderVo"/>
        <where>  
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="consultantId != null "> and consultant_id = #{consultantId}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="consultingTime != null "> and consulting_time = #{consultingTime}</if>
            <if test="subscribeTime != null "> and subscribe_time = #{subscribeTime}</if>
            <if test="price != null "> and price = #{price}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="orderNum != null  and orderNum != ''"> and order_num = #{orderNum}</if>
            <if test="orderTime != null "> and order_time = #{orderTime}</if>
        </where>
    </select>
    
    <select id="selectPsyConsultingOrderById"  resultMap="PsyConsultingOrderResult">
        <include refid="selectPsyConsultingOrderVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertPsyConsultingOrder" parameterType="PsyConsultingOrder" useGeneratedKeys="true" keyProperty="id">
        insert into psy_consulting_order
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="consultantId != null">consultant_id,</if>
            <if test="type != null">type,</if>
            <if test="consultingTime != null">consulting_time,</if>
            <if test="subscribeTime != null">subscribe_time,</if>
            <if test="price != null">price,</if>
            <if test="status != null">status,</if>
            <if test="orderNum != null">order_num,</if>
            <if test="orderTime != null">order_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="consultantId != null">#{consultantId},</if>
            <if test="type != null">#{type},</if>
            <if test="consultingTime != null">#{consultingTime},</if>
            <if test="subscribeTime != null">#{subscribeTime},</if>
            <if test="price != null">#{price},</if>
            <if test="status != null">#{status},</if>
            <if test="orderNum != null">#{orderNum},</if>
