<mapper namespace="com.renxin.gauge.mapper.PsyConsultingOrderMapper">
    
    <resultMap type="PsyConsultingOrder" id="PsyConsultingOrderResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="consultantId"    column="consultant_id"    />
        <result property="type"    column="type"    />
        <result property="consultingTime"    column="consulting_time"    />
        <result property="subscribeTime"    column="subscribe_time"    />
        <result property="price"    column="price"    />
