<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.renxin.gauge.mapper.PsyGaugeScoreSettingMapper">
    
    <resultMap type="com.renxin.gauge.domain.PsyGaugeScoreSetting" id="PsyGaugeScoreSettingResult">
        <result property="id"    column="id"    />
        <result property="gaugeId"    column="gauge_id"    />
        <result property="start"    column="start"    />
        <result property="end"    column="end"    />
        <result property="proposal"    column="proposal"    />
        <result property="result"    column="result"    />
        <result property="lat"    column="lat"    />
        <result property="memo1"    column="memo1"    />
        <result property="memo2"    column="memo2"    />
        <result property="memo3"    column="memo3"    />
        <result property="memo4"    column="memo4"    />
        <result property="memo5"    column="memo5"    />
        <result property="memo6"    column="memo6"    />
        <result property="memo7"    column="memo7"    />
        <result property="params"    column="params"    />
    </resultMap>

    <sql id="selectPsyGaugeScoreSettingVo">
        select id, gauge_id, start, end, lat, proposal,result,memo1,memo2,memo3,memo4,memo5,memo6,memo7,params from psy_gauge_score_setting
    </sql>

    <select id="selectPsyGaugeScoreSettingList" parameterType="com.renxin.gauge.domain.PsyGaugeScoreSetting" resultMap="PsyGaugeScoreSettingResult">
        <include refid="selectPsyGaugeScoreSettingVo"/>
        <where>  
            <if test="gaugeId != null "> and gauge_id = #{gaugeId}</if>
            <if test="start != null "> and start = #{start}</if>
            <if test="end != null "> and end = #{end}</if>
            <if test="lat != null "> and end = #{lat}</if>
            <if test="proposal != null  and proposal != ''"> and proposal = #{proposal}</if>
        </where>
    </select>
    <select id="selectPsyGaugeScoreSettingListByIds" parameterType="String" resultMap="PsyGaugeScoreSettingResult">
        <include refid="selectPsyGaugeScoreSettingVo"/>
        where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>

    <select id="selectPsyGaugeScoreSettingById"  resultMap="PsyGaugeScoreSettingResult">
        <include refid="selectPsyGaugeScoreSettingVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertPsyGaugeScoreSetting" parameterType="com.renxin.gauge.domain.PsyGaugeScoreSetting" useGeneratedKeys="true" keyProperty="id">
        insert into psy_gauge_score_setting
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="gaugeId != null">gauge_id,</if>
            <if test="start != null">start,</if>
            <if test="end != null">end,</if>
            <if test="lat != null">lat,</if>
            <if test="result != null">result,</if>
            <if test="proposal != null">proposal,</if>
            <if test="memo1 != null">memo1,</if>
            <if test="memo2 != null">memo2,</if>
            <if test="memo3 != null">memo3,</if>
            <if test="memo4 != null">memo4,</if>
            <if test="memo5 != null">memo5,</if>
            <if test="memo6 != null">memo6,</if>
            <if test="memo7 != null">memo7,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="gaugeId != null">#{gaugeId},</if>
            <if test="start != null">#{start},</if>
            <if test="end != null">#{end},</if>
            <if test="lat != null">#{lat},</if>
            <if test="result != null">#{result},</if>
            <if test="proposal != null">#{proposal},</if>
            <if test="memo1 != null">#{memo1},</if>
            <if test="memo2 != null">#{memo2},</if>
            <if test="memo3 != null">#{memo3},</if>
            <if test="memo4 != null">#{memo4},</if>
            <if test="memo5 != null">#{memo5},</if>
            <if test="memo6 != null">#{memo6},</if>
            <if test="memo7 != null">#{memo7},</if>
         </trim>
    </insert>

    <update id="updatePsyGaugeScoreSetting" parameterType="com.renxin.gauge.domain.PsyGaugeScoreSetting">
        update psy_gauge_score_setting
        <trim prefix="SET" suffixOverrides=",">
            <if test="gaugeId != null">gauge_id = #{gaugeId},</if>
            <if test="start != null">start = #{start},</if>
            <if test="end != null">end = #{end},</if>
            <if test="lat != null">lat = #{lat},</if>
            <if test="proposal != null">proposal = #{proposal},</if>
            <if test="memo1 != null">memo1 = #{memo1},</if>
            <if test="memo2 != null">memo2 = #{memo2},</if>
            <if test="memo3 != null">memo3 = #{memo3},</if>
            <if test="memo4 != null">memo4 = #{memo4},</if>
            <if test="memo5 != null">memo5 = #{memo5},</if>
            <if test="memo6 != null">memo6 = #{memo6},</if>
            <if test="memo7 != null">memo7 = #{memo7},</if>
            <if test="result != null">result = #{result},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deletePsyGaugeScoreSettingById" >
        delete from psy_gauge_score_setting where id = #{id}
    </delete>

    <delete id="deletePsyGaugeScoreSettingByIds" parameterType="String">
        delete from psy_gauge_score_setting where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="selectPsyGaugeScoreSettingByGaugeId" parameterType="java.util.Map" resultMap="PsyGaugeScoreSettingResult">
         select t1.id, t1.gauge_id, t1.start, t1.end, t1.lat, t1.proposal,t1.result,t1.memo1,t1.memo2,t1.memo3,t1.memo4,t1.memo5,t1.memo6,t1.memo7,t1.params from psy_gauge_score_setting t1
        where t1.gauge_id = (select gauge_id from psy_order where id=#{orderId} )
          and ((#{score} >= t1.start AND t1.end >= #{score}) or (t1.lat = #{score}))
    </select>
</mapper>