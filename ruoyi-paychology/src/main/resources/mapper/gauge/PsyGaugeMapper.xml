<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.renxin.gauge.mapper.PsyGaugeMapper">
    
    <resultMap type="com.renxin.gauge.domain.PsyGauge" id="PsyGaugeResult">
        <result property="id"    column="id"    />
        <result property="title"    column="title"    />
        <result property="subtitle"    column="subtitle"    />
        <result property="headPicture"    column="head_picture"    />
        <result property="introduce"    column="introduce"    />
        <result property="gaugeClass"    column="gauge_class"    />
        <result property="gaugeNum"    column="gauge_num"    />
        <result property="price"    column="price"    />
        <result property="num"    column="num"    />
        <result property="gaugeRatio"    column="gauge_ratio"    />
        <result property="gaugeScore"    column="gauge_score"    />
        <result property="wrong"    column="wrong"    />
        <result property="radar"    column="radar"    />
        <result property="type"    column="type"    />
        <result property="createTime"    column="create_time"    />
        <result property="createBy"      column="create_by"      />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"      column="update_by"      />
        <result property="updateTime"    column="update_time"    />
        <result property="gaugeDes"    column="gauge_des"    />
        <result property="listShowPicture"    column="list_show_picture"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="analysePrice"    column="analyse_price"    />

    </resultMap>

    <sql id="selectPsyGaugeVo">
        select id, title, subtitle, head_picture, introduce, gauge_class, gauge_num, price, num, gauge_ratio, gauge_score, wrong, radar, 
               type,create_by, create_time, update_by, update_time, remark,gauge_des,list_show_picture, analyse_price  from psy_gauge
    </sql>

    <select id="selectPsyGaugeList" parameterType="com.renxin.gauge.domain.PsyGauge" resultMap="PsyGaugeResult">
        <include refid="selectPsyGaugeVo"/>
        <where>
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="subtitle != null  and subtitle != ''"> and subtitle = #{subtitle}</if>
            <if test="headPicture != null  and headPicture != ''"> and head_picture = #{headPicture}</if>
            <if test="introduce != null  and introduce != ''"> and introduce = #{introduce}</if>
            <if test="gaugeClass != null "> and gauge_class = #{gaugeClass}</if>
            <if test="gaugeNum != null "> and gauge_num = #{gaugeNum}</if>
            <if test="radar != null "> and radar = #{radar}</if>
            <if test="wrong != null "> and wrong = #{wrong}</if>
            <if test="price != null "> and price = #{price}</if>
            <if test="gaugeRatio != null "> and gauge_ratio = #{gaugeRatio}</if>
            <if test="gaugeScore != null "> and gauge_score = #{gaugeScore}</if>
            <if test="num != null "> and num = #{num}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="gaugeDes != null "> and gauge_des = #{gaugeDes}</if>
            <if test="analysePrice != null "> and analyse_price = #{analysePrice}</if>
            <if test="idList != null and idList.size > 0">
                and id in
                <foreach collection="idList" open="(" close=")" separator="," item="id">
                    #{id}
                </foreach>
            </if>
        </where>
    </select>
    
    <select id="selectPsyGaugeById"  resultMap="PsyGaugeResult">
        <include refid="selectPsyGaugeVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertPsyGauge" parameterType="com.renxin.gauge.domain.PsyGauge" useGeneratedKeys="true" keyProperty="id">
        insert into psy_gauge
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="title != null">title,</if>
            <if test="subtitle != null">subtitle,</if>
            <if test="headPicture != null">head_picture,</if>
            <if test="introduce != null">introduce,</if>
            <if test="gaugeClass != null">gauge_class,</if>
            <if test="gaugeNum != null">gauge_num,</if>
            <if test="gaugeRatio != null">gauge_ratio,</if>
            <if test="gaugeScore != null">gauge_score,</if>
            <if test="radar != null">radar,</if>
            <if test="wrong != null">wrong,</if>
            <if test="price != null">price,</if>
            <if test="num != null">num,</if>
            <if test="type != null">type,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="gaugeDes != null">gauge_des,</if>
            <if test="listShowPicture != null">list_show_picture,</if>
            <if test="analysePrice != null">analyse_price,</if>


         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="title != null">#{title},</if>
            <if test="subtitle != null">#{subtitle},</if>
            <if test="headPicture != null">#{headPicture},</if>
            <if test="introduce != null">#{introduce},</if>
            <if test="gaugeClass != null">#{gaugeClass},</if>
            <if test="gaugeNum != null">#{gaugeNum},</if>
            <if test="gaugeRatio != null">#{gaugeRatio},</if>
            <if test="gaugeScore != null">#{gaugeScore},</if>
            <if test="radar != null">#{radar},</if>
            <if test="wrong != null">#{wrong},</if>
            <if test="price != null">#{price},</if>
            <if test="num != null">#{num},</if>
            <if test="type != null">#{type},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="gaugeDes != null">#{gaugeDes},</if>
            <if test="listShowPicture != null">#{listShowPicture},</if>
            <if test="analysePrice != null">#{analysePrice},</if>
         </trim>
    </insert>

    <update id="updatePsyGauge" parameterType="com.renxin.gauge.domain.PsyGauge">
        update psy_gauge
        <trim prefix="SET" suffixOverrides=",">
            <if test="title != null">title = #{title},</if>
            <if test="subtitle != null">subtitle = #{subtitle},</if>
            <if test="headPicture != null">head_picture = #{headPicture},</if>
            <if test="introduce != null">introduce = #{introduce},</if>
            <if test="gaugeClass != null">gauge_class = #{gaugeClass},</if>
            <if test="gaugeNum != null">gauge_num = #{gaugeNum},</if>
            <if test="gaugeRatio != null ">gauge_ratio = #{gaugeRatio},</if>
            <if test="gaugeScore != null ">gauge_score = #{gaugeScore},</if>
            <if test="radar != null ">radar = #{radar},</if>
            <if test="wrong != null ">wrong = #{wrong},</if>
            <if test="price != null">price = #{price},</if>
            <if test="num != null">num = #{num},</if>
            <if test="type != null">type = #{type},</if>
            <if test="updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
            <if test="gaugeDes != null">gauge_des = #{gaugeDes},</if>
            <if test="listShowPicture != null">list_show_picture = #{listShowPicture},</if>
            <if test="analysePrice != null">analyse_price = #{analysePrice},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deletePsyGaugeById" >
        update psy_gauge
        set del_flag = 1
        where id = #{id}
    </delete>

    <delete id="deletePsyGaugeByIds" parameterType="String">
        update psy_gauge
        set del_flag = 1
        where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>