    <sql id="selectPsyConsultantOrderVo">
        select id, order_no, server_id, server_type, server_name, status, pay_consultant_id, pay_consultant_name, pay_amount, pay_datetime, pay_status, 
               remark, del_flag, create_by, create_time, update_by, update_time,pay_id, pay_type,original_price,coupon_no, ali_pay_param, wx_pay_param, 
               consultant_ratio, consultant_price, member_type
        from psy_consultant_order
    </sql>

    <select id="selectPsyConsultantOrderList" parameterType="PsyConsultantOrder" resultMap="PsyConsultantOrderResult">
        <include refid="selectPsyConsultantOrderVo"/>
        <where>  
            del_flag = 0
            <if test="orderNo != null  and orderNo != ''"> and order_no like concat('%',#{orderNo},'%') </if>
            <if test="serverId != null  and serverId != ''"> and server_id = #{serverId}</if>
            <if test="serverType != null and serverType != ''"> and server_type = #{serverType}</if>
            <if test="serverName != null  and serverName != ''"> and server_name like concat('%', #{serverName}, '%')</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="payConsultantId != null and payConsultantId != ''"> and pay_consultant_id = #{payConsultantId}</if>
            <if test="payConsultantName != null  and payConsultantName != ''"> and pay_consultant_name like concat('%', #{payConsultantName}, '%')</if>
            <if test="payAmount != null "> and pay_amount = #{payAmount}</if>
            <if test="payDatetime != null "> and pay_datetime = #{payDatetime}</if>
            <if test="payStatus != null  and payStatus != ''"> and pay_status = #{payStatus}</if>
            <if test="payType != null  and payType != ''"> and pay_type = #{payType}</if>
            <if test="createBy != null  and createBy != ''"> and create_by = #{createBy}</if>
             <if test="serverTypeList != null and serverTypeList.size > 0">
                 and server_type in
                 <foreach collection="serverTypeList" open="(" close=")" separator="," item="item">
                     #{item}
                 </foreach>
             </if>
        </where>
        order by create_time desc
    </select>
    
    <select id="selectPsyConsultantOrderByOrderNo" parameterType="String" resultMap="PsyConsultantOrderResult">
        <include refid="selectPsyConsultantOrderVo"/>
