            <if test="paralegalB != null  and paralegalB != ''"> and paralegal_b = #{paralegalB}</if>
            <if test="mediator != null  and mediator != ''"> and mediator = #{mediator}</if>
            <if test="firstInstanceFileids != null  and firstInstanceFileids != ''"> and first_instance_fileids = #{firstInstanceFileids}</if>
            <if test="secondInstanceFileids != null  and secondInstanceFileids != ''"> and second_instance_fileids = #{secondInstanceFileids}</if>
            <if test="firstExecutionFileids != null  and firstExecutionFileids != ''"> and first_execution_fileids = #{firstExecutionFileids}</if>
            <if test="secondExecutionFileids != null  and secondExecutionFileids != ''"> and second_execution_fileids = #{secondExecutionFileids}</if>
            <if test="claimStatus != null  and claimStatus != ''"> and claim_status = #{claimStatus}</if>
            <if test="isReturn != null  and isReturn != ''"> and is_return = #{isReturn}</if>
            <if test="arrears != null "> and arrears = #{arrears}</if>
            <if test="arrearsYes != null "> and arrears_yes = #{arrearsYes}</if>
            <if test="arrearsNo != null "> and arrears_no = #{arrearsNo}</if>
            <if test="caseStatus != null  and caseStatus != ''"> and case_status = #{caseStatus}</if>
            <if test="dataRoleUserids != null  and dataRoleUserids != ''"> and data_role_userids = #{dataRoleUserids}</if>
            <if test="isDelete != null  and isDelete != ''"> and is_delete = #{isDelete}</if>
        </where>
    </select>

    <select id="selectCaseInfoById" parameterType="Long" resultMap="CaseInfoResult">
        <include refid="selectCaseInfoVo"/>
        where id = #{id}
    </select>

    <insert id="insertCaseInfo" parameterType="CaseInfo">
        insert into rxh_case_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
