    <sql id="selectCaseInfoVo">
        select id, plaintiff, defendant, defendant_phone, court, case_no, cooperative_company, undertake_lawyer_a, undertake_lawyer_b, paralegal_a, paralegal_b, mediator, first_instance_fileids, second_instance_fileids, first_execution_fileids, second_execution_fileids, claim_status, is_return, arrears, arrears_yes, arrears_no, case_status, create_by, create_time, update_by, update_time, remark, data_role_userids, is_delete from rxh_case_info
    </sql>

    <select id="selectCaseInfoList" parameterType="CaseInfo" resultMap="CaseInfoResult">
        <include refid="selectCaseInfoVo"/>
        <where>
            <if test="plaintiff != null  and plaintiff != ''"> and plaintiff = #{plaintiff}</if>
            <if test="defendant != null  and defendant != ''"> and defendant = #{defendant}</if>
            <if test="defendantPhone != null  and defendantPhone != ''"> and defendant_phone = #{defendantPhone}</if>
            <if test="court != null  and court != ''"> and court = #{court}</if>
            <if test="caseNo != null  and caseNo != ''"> and case_no = #{caseNo}</if>
            <if test="cooperativeCompany != null  and cooperativeCompany != ''"> and cooperative_company = #{cooperativeCompany}</if>
